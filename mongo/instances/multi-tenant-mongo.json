{
    "$schema": "https://facets-cloud.github.io/facets-schemas/schemas/mongodb/mongodb.schema.json",
    "version":"0.2",
    "flavor": "k8s",
    "kind": "mongo",
    "metadata": {
        "name": "multi-tenant-mongo"
    },
    "disabled": true,
    "spec": {
        "authenticated": false,
        "mongodb_version": "6.0.1",
        "size": {
            "cpu": "0.5Gi",
            "memory": "512Mi",
            "replica_count": 1,
            "volume": "20Gi"
        }
    }
}